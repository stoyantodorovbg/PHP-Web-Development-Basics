CREATE TABLE `employees_phone` (
	`id` INT NOT NULL,
	`phone_type` VARCHAR(50) NOT NULL,
	`phone` VARCHAR(50) NOT NULL,
	CONSTRAINT `phone_id` FOREIGN KEY (`id`) REFERENCES `employees` (`id`)
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;
