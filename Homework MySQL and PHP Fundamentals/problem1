CREATE TABLE `employees_emails` (
	`id` INT NULL,
	`email_type` VARCHAR(50) NOT NULL,
	`email` VARCHAR(50) NOT NULL,
	CONSTRAINT `email_id` FOREIGN KEY (`id`) REFERENCES `employees` (`id`)
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;
